{
  "name": "nuxt-clean",
  "version": "1.0.0",
  "description": "My pioneering Nuxt.js project",
  "author": "Adri√† Fontcuberta",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts src/ui --tsconfig . --port 3000",
    "build": "nuxt-ts build src/ui --tsconfig .",
    "start": "nuxt-ts start src/ui --tsconfig .",
    "generate": "nuxt-ts generate src/ui --tsconfig .",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint src/**/*.{vue,css}",
    "format": "prettier '**/*.{js,ts,vue}' --write && stylelint '**/*.{css,vue}' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e:dev": "cypress open",
    "test:e2e:run": "cypress run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "dependencies": {
    "@holaluz/margarita": "^8.0.0",
    "@nuxt/typescript-runtime": "^2.0.0",
    "axios": "^0.21.1",
    "nuxt": "^2.14.12",
    "nuxt-i18n": "^6.17.0",
    "vee-validate": "^3.4.5"
  },
  "devDependencies": {
    "@holaluz/npm-scripts": "^5.1.0",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/vue": "^5.6.1",
    "@types/cypress-cucumber-preprocessor": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cypress": "^6.2.1",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-plugin-vue": "^7.4.1",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.3",
    "lodash.merge": "^4.6.2",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "ts-jest": "^26.4.1",
    "vue-jest": "^4.0.0-rc.1"
  }
}
