    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.2",
    "lodash.merge": "^4.6.2",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
