{
  "name": "nuxt-clean",
  "version": "1.0.0",
  "description": "My pioneering Nuxt.js project",
  "author": "Adri√† Fontcuberta",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts src/ui --tsconfig .",
    "build": "nuxt-ts build src/ui --tsconfig .",
    "start": "nuxt-ts start src/ui --tsconfig .",
    "generate": "nuxt-ts generate src/ui --tsconfig .",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "format": "prettier '**/*.{js,ts,vue}' --write",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "nuxt": "^2.14.7"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.7",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config-typescript": "^4.0.0",
    "@nuxtjs/eslint-module": "^3.0.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "vue-jest": "^4.0.0-rc.0"
  }
}
